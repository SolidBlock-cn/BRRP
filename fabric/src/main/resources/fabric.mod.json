{
  "schemaVersion": 1,
  "id": "better_runtime_resource_pack",
  "version": "${version}",
  "name": "Better Runtime Resource Pack",
  "description": "BRRP is a library mod that can generate resource packs at runtime. It is an extension of ARRP mod, which fixes some issues and adds some enhancements. The mod is trying the best to make it compatible with ARRP-dependent mods. If you have installed BRRP you do not have to install ARRP again.",
  "authors": [
    "HalfOf2",
    "SolidBlock"
  ],
  "contact": {
    "sources": "https://github.com/SolidBlock-cn/BRRP",
    "issues": "https://github.com/SolidBlock-cn/BRRP/issues"
  },
  "license": "MPLv2",
  "icon": "assets/brrp/icon.png",
  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "pers.solid.brrp.fabric.BRRPPrelaunch"
    ],
    "main": [
      "pers.solid.brrp.fabric.BRRPInitialize"
    ]
  },
  "mixins": [
    "arrp.mixins.json",
    "brrp.mixins.json",
    "brrp-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.0",
    "minecraft": ">=1.19.3"
  },
  "custom": {
    "modmenu": {
      "badges": ["library"]
    },
    "loom:injected_interfaces": {
      "net/minecraft/class_2960": ["net/devtech/arrp/IdentifierExtension"]
    }
  },
  "provides": ["advanced_runtime_resource_pack"]
}
